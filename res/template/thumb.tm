<a class='tooltip' href='/?p={{.dirPath}}&c={{.imgPath}}'>
    <div class='thumb-outer'>
        <img src='/thumb/?p={{.thumbName}}'></img>
        <span>
            {{.thumbName}}
        </span>
        <div class='{{.class}}'></div>
        <div class='mark'>
            <a class='file-download' href='/file/?p={{.imgPath}}'>
                <div class='download'></div>
            </a>
        </div>
    </div>
</a>